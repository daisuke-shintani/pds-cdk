{
  "name": "pds-cdk",
  "version": "0.1.0",
  "bin": {
    "pds-cdk": "bin/pds-cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "test:fga": "jest ./*/fineGrained.test.ts",
    "test:snap": "jest ./*/snapshot.test.ts",
    "prepare": "husky .husky",
    "format": "prettier --write \"*/**/*.ts\"",
    "format:ci": "prettier --check \"*/**/*.ts\"",
    "lint": "eslint \"*/**/*.ts\" --max-warnings 0 --fix",
    "lint:ci": "eslint \"*/**/*.ts\" --max-warnings 0",
    "format-lint": "prettier --write \"*/**/*.ts\" && eslint \"*/**/*.ts\" --max-warnings 0",
    "md-format-lint": "prettier --write \"../**/*.md\" --tab-width 4 && markdownlint-cli2 \"../**/*.md\"",
    "depcheck": "depcheck"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "20.12.7",
    "aws-cdk": "2.143.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.5",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "depcheck": "^1.4.7",
    "esbuild": "^0.21.5",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.2.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "standard-version": "^9.5.0",
    "markdownlint-cli2": "^0.13.0",
    "cdk-nag": "^2.28.153"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.592.0",
    "@aws-sdk/client-kinesis": "^3.592.0",
    "aws-cdk-lib": "2.143.1",
    "constructs": "^10.0.0",
    "esbuild": "^0.21.5",
    "source-map-support": "^0.5.21"
  },
  "lint-staged": {
    "*.{ts,tsx}": "npm run format-lint",
    "../**/*.md": "npm run md-format-lint"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
